#include "WrapperFinder.h"int main(void){		err = FSMakeFSSpec(0, 0, CtoPstr(app_path), &filespec); // Make FSSpec from path	if (err != noErr) {		printf("Failed to make FSSpec. (error %d).\n",err);	}		// Set up parameters for LaunchApplication	launch_params.launchBlockID = extendedBlock; 	launch_params.launchEPBLength = extendedBlockLen;	launch_params.launchFileFlags = 0;	launch_params.launchControlFlags = launchContinue + launchNoFileFlags;	launch_params.launchAppSpec = &filespec;	//launch_params.launchProcessSN = &process_sn;		err = LaunchApplication(&launch_params);	if (err != noErr) {		printf("Failed to launch application. (error %d).\n",err);	}		// Wait for process to quit	process_sn = launch_params.launchProcessSN;	/*while (process_result != -600){		process_result = GetProcessInformation(&process_sn,&process_info);		//printf("process status is (error %d)\n", process_result);	};*/					//ShutDwnPower();		return 0;}